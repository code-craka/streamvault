{
  "name": "streamvault",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm exec eslint . --fix",
    "lint:check": "pnpm exec eslint .",
    "format": "pnpm exec prettier --write .",
    "format:check": "pnpm exec prettier --check .",
    "type-check": "pnpm exec tsc --noEmit",
    "test": "pnpm exec jest",
    "test:watch": "pnpm exec jest --watch",
    "test:coverage": "pnpm exec jest --coverage",
    "test:e2e": "pnpm exec playwright test",
    "prepare": "husky install",
    "firebase:setup": "pnpm exec node scripts/setup-firebase.js",
    "firebase:test": "curl -X GET http://localhost:3000/api/firebase/config",
    "firebase:deploy": "pnpm exec firebase deploy",
    "firebase:rules": "pnpm exec firebase deploy --only firestore:rules",
    "firebase:indexes": "pnpm exec firebase deploy --only firestore:indexes",
    "firebase:emulator": "pnpm exec firebase emulators:start",
    "gcs:setup": "pnpm exec node scripts/setup-gcs.js",
    "gcs:configure": "pnpm exec node scripts/configure-gcs-bucket.js",
    "gcs:test": "curl -X GET http://localhost:3000/api/storage/test",
    "gcs:test-direct": "pnpm exec node scripts/test-gcs-direct.js",
    "storage:test": "pnpm gcs:test"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.34.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^11.0.0",
    "hls.js": "^1.4.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.400.0",
    "next": "15.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stripe": "^18.4.0",
    "svix": "^1.74.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.15.0"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "overrides": {
      "undici": ">=6.21.2"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19",
        "eslint": "9"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json,css}": [
      "pnpm exec prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "pnpm exec eslint --fix"
    ]
  }
}